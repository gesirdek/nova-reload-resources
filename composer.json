{
    "name": "gesirdek/nova-reload-resources",
    "description": "Creates a Reload Resources button and a autoreload option",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Tanmuhittin\\ReloadResources\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanmuhittin\\ReloadResources\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
